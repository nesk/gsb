<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <appSettings>
    <add key="db-type" value="mysql"/>
    
    <add key="mssql-lastid" value="scope_identity"/>
    <add key="mysql-lastid" value="last_insert_id"/>
  </appSettings>
  
  <connectionStrings>
    <add name="mssql" providerName="System.Data.SqlClient"
         connectionString="server=localhost; database=gsbV2; trusted_connection=sspi" />
    <add name="mysql" providerName="MySql.Data.MySqlClient"
         connectionString="Server=localhost; Database=gsbV2; Uid=root;" />
  </connectionStrings>
</configuration>